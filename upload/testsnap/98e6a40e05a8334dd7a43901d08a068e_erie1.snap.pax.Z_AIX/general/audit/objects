
/etc/security/environ:
	w = "S_ENVIRON_WRITE"

/etc/security/group:
	w = "S_GROUP_WRITE"

/etc/security/limits:
	w = "S_LIMITS_WRITE"

/etc/security/login.cfg:
	w = "S_LOGIN_WRITE"

/etc/security/passwd:
	r = "S_PASSWD_READ"
	w = "S_PASSWD_WRITE"

/etc/security/user:
	w = "S_USER_WRITE"

/etc/security/audit/config:
	w = "AUD_CONFIG_WR"

