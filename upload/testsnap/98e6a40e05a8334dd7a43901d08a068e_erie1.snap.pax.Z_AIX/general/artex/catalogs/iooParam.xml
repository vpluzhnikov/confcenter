<?xml version="1.0" encoding="UTF-8"?>
<Catalog id="iooParam" version="2.1">

	<ShortDescription><NLSCatalog catalog="artexcat.cat" setNum="22" msgNum="1">ioo parameters</NLSCatalog></ShortDescription>
	<Description><NLSCatalog catalog="artexcat.cat" setNum="22" msgNum="2">Parameter definition for the ioo command</NLSCatalog></Description>

	<CfgMethod id="ioo">
		<Get type="current">
			<Command>/usr/sbin/ioo -aF</Command>
			<Filter>/usr/bin/grep -v '= n/a$'</Filter>
			<Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
		</Get>

		<Get type="nextboot">
			<Command>/usr/sbin/ioo -r -aF</Command>
			<Filter>/usr/bin/grep -v '= n/a$'</Filter>
			<Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
		</Get>

		<Set type="permanent">
			<!-- echo yes is to answer prompt to set restricted variables if required
			     change if a parameter is ever introduced that requires a bosboot -->
			<Command>TMP_SET_ARGS=%a; if [[ -n $TMP_SET_ARGS ]]; then echo yes | /usr/sbin/ioo -p $TMP_SET_ARGS; fi</Command>
			<Argument>`if [ %v1 != 'n/a' ]; then echo ' -o '%p=%v1; fi`</Argument>
		</Set>

		<Set type="nextboot">
			<Command>TMP_SET_ARGS=%a; if [[ -n $TMP_SET_ARGS ]]; then echo yes | /usr/sbin/ioo -r $TMP_SET_ARGS; fi</Command>
			<Argument>`if [ %v1 != 'n/a' ]; then echo ' -o '%p=%v1; fi`</Argument>

		</Set>
	</CfgMethod>

