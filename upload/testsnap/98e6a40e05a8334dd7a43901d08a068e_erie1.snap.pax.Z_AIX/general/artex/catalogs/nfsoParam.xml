<?xml version="1.0" encoding="UTF-8"?>
<Catalog id="nfsoParam" version="2.1">

  <ShortDescription><NLSCatalog catalog="artexcat.cat" setNum="30" msgNum="1">nfso parameters</NLSCatalog></ShortDescription>
 
  <Description><NLSCatalog catalog="artexcat.cat" setNum="30" msgNum="2">Parameter definition for the 'nfso' command</NLSCatalog></Description>
 
  <CfgMethod id="nfso">
  
    <Get type="current">
      <Command>/usr/sbin/nfso -a -F</Command>
      <Filter>/usr/bin/grep -v '= n/a$'</Filter>
      <Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
    </Get>

    <Get type="nextboot">
      <Command>/usr/sbin/nfso -r -a -F</Command>
      <Filter>/usr/bin/grep -v '= n/a$'</Filter>
      <Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
    </Get>
  
    <Set type="permanent">
      <!--do not set parameter if value is 'n/a' -->
      <!-- echo yes is to answer prompt to set restricted variables if required
	   change if a parameter is ever introduced that requires a bosboot -->
      <Command>TMP_SET_ARGS=%a; if [[ -n $TMP_SET_ARGS ]]; then echo yes | /usr/sbin/nfso -p $TMP_SET_ARGS; fi</Command>
      <Argument>`if [ %v1 != 'n/a' ]; then echo ' -o '%p=%v1; fi`</Argument>
    </Set>

    <Set type="nextboot">
      <!--do not set parameter if value is 'n/a' -->
      <Command>TMP_SET_ARGS=%a; if [[ -n $TMP_SET_ARGS ]]; then echo yes | /usr/sbin/nfso -r $TMP_SET_ARGS; fi</Command>
      <Argument>`if [ %v1 != 'n/a' ]; then echo ' -o '%p=%v1; fi`</Argument>
    </Set>

  </CfgMethod>

  <ParameterDef name="client_delegation" type="boolean" cfgmethod="nfso">
