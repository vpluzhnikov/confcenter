<?xml version="1.0" encoding="UTF-8"?>
<Catalog id="privfileParam" version="2.0">

 <ShortDescription><NLSCatalog catalog="artexcat.cat" setNum="36" msgNum="1">RBAC (Role-Based Access Control) Privileged Files.</NLSCatalog></ShortDescription>
 
 <Description><NLSCatalog catalog="artexcat.cat" setNum="36" msgNum="2">The privileged file database provides a method to use authorizations to determine access to system configuration files.</NLSCatalog></Description>
 
 <CfgMethod id="privfile">
  <Get type="nextboot">
   <Command>/usr/bin/cat /etc/security/privfiles</Command>
  </Get>

  <Get type="current">
   <Command>/usr/bin/cat /etc/security/privfiles</Command>
  </Get>

  <Set type="permanent">
   <Command>/usr/bin/cat &gt; /etc/security/privfiles</Command>
   <Stdin>%v1</Stdin>
  </Set>
 </CfgMethod>

 <ParameterDef name="privatefiles" type="binary" cfgmethod="privfile">
  <Description><NLSCatalog catalog="artexcat.cat" setNum="36" msgNum="3">The /etc/security/privfiles file is an ASCII stanza file that contains privileged files and their security attributes.</NLSCatalog></Description>
 </ParameterDef>
</Catalog>
