<?xml version="1.0" encoding="UTF-8"?>
<Catalog id="ewlmParam" version="2.0">

 <ShortDescription><NLSCatalog catalog="artexcat.cat" setNum="18" msgNum="1">Enterprise Workload Manager.</NLSCatalog></ShortDescription>
 
 <Description><NLSCatalog catalog="artexcat.cat" setNum="18" msgNum="2">Enterprise Workload Manager (EWLM) is a robust performance management tool that allows you to monitor and manage work that runs within your environment. EWLM is robust in that it allows you to define specific performance goals. In addition, EWLM allows you to monitor application-level transactions separate from operating system processes. Furthermore, EWLM allows you to assign performance goals to specific work.</NLSCatalog></Description>
 
 <CfgMethod id="ewlmconfig">
  <Get type="current">
   <Command>/usr/sbin/ewlmcfg -C ALL</Command>
   <Filter>awk -F: '{ N = split(substr($0, 2), NAME); getline; split($0, VALUE) } END { for (i=1; i&lt;=N; i++) { print NAME[i] " = " VALUE[i] } }'</Filter>
   <Mask name="1" value="2">(.*) = (.*)</Mask>
  </Get>

  <Get type="nextboot">
   <Command>/usr/sbin/ewlmcfg -C ALL</Command>
   <Filter>awk -F: '{ N = split(substr($0, 2), NAME); getline; split($0, VALUE) } END { for (i=1; i&lt;=N; i++) { print NAME[i] " = " VALUE[i] } }'</Filter>
   <Mask name="1" value="2">(.*) = (.*)</Mask>
  </Get>

  <Set type="permanent">
   <Command>/usr/sbin/ewlm_chconfig limits %a </Command>
   <Argument> -a %p=%v1</Argument>
  </Set>
 </CfgMethod>

 <ParameterDef name="ewlm" type="boolean">

  <Description><NLSCatalog catalog="artexcat.cat" setNum="18" msgNum="3">Enablement of EWLM management tool.</NLSCatalog></Description>
  <Get type="current">
   <Command>LANG=C;/usr/sbin/ewlmcfg -q 2&gt;&amp;1</Command>
   <Filter>sed "2,4d" | cut -d" " -f 3 | sed -e "s/enabled./1/" | sed -e "s/disabled./0/" | tr -d '\n'</Filter>
  </Get>

  <Get type="nextboot">
   <Command>ODMDIR=/etc/objrepos;/usr/bin/odmget Config_Rules</Command>
   <Filter>grep ewlmcfg &gt;/dev/null 2&gt;&amp;1; if [ $? -eq 0 ]; then echo "1"; else echo "0";fi | tr -d '\n'</Filter>
  </Get>
  
  <Set type="current">
   <Command>/usr/sbin/ewlmcfg %a</Command>
   <Argument>`if [ ''%v1 = '0' ]; then echo '-u'; else echo '-c'; fi`; echo " &gt;/dev/null 2&gt;&amp;1;</Argument>
  </Set>

  <Set type="nextboot">
   <Command>ODMDIR=/etc/objrepos;/usr/bin/odmdelete -o Config_Rules -q "rule LIKE *ewlmcfg*" &gt;/dev/null 2&gt;&amp;1;RC=$?;if [ ''%v1 = '1' ]; then echo '
Config_Rules:
	phase = 2
	seq = 0
	rule = "/usr/bin/ewlmcfg -c"
' | /usr/bin/odmadd &gt;/dev/null 2&gt;&amp;1;exit $?; else exit $RC; fi</Command>
  </Set>
 </ParameterDef>

