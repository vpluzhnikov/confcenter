	<ParameterDef name="pp_wterm" type="string" cfgmethod="shconf_prio" reboot="false"> 
		<Description><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="10">Terminal Device</NLSCatalog></Description>
		<Get type="current">
			<Command>LANG=C /usr/sbin/shconf -D -l prio</Command>
			<Filter>/usr/bin/grep wterm | cut -c 12-80 | cut -d " " -f 1</Filter>
			<Mask value="1">(.*)</Mask>
		</Get>
		
		<Get type="nextboot">
			<Command>LANG=C /usr/sbin/shconf -D -l prio</Command>
			<Filter>/usr/bin/grep wterm | cut -c 12-80 | cut -d " " -f 1</Filter>
