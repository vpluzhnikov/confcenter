<?xml version="1.0" encoding="UTF-8"?>
<Catalog id="shconfParam" version="2.0">

	<ShortDescription><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="1">shconf parameters</NLSCatalog></ShortDescription>
 
	<Description><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="2">Parameter definition for the 'shconf' command</NLSCatalog></Description>
 
	<CfgMethod id="shconf_prio">  
		<Get type="current">
			<Command>LANG=C /usr/sbin/shconf -E -l prio</Command>
			<Filter>/usr/bin/sed -e 's/disable/0/;s/enable/1/'</Filter>
			<Mask name="1" value="2">([^ ]*)[ ]+([^ ]*)</Mask>
		</Get>
		
		<Get type="nextboot">
			<Command>LANG=C /usr/sbin/shconf -E -l prio</Command>
			<Filter>/usr/bin/sed -e 's/disable/0/;s/enable/1/'</Filter>
			<Mask name="1" value="2">([^ ]*)[ ]+([^ ]*)</Mask>
		</Get>
  
		<Set type="permanent">
			<Command>LANG=C /usr/sbin/shconf -l prio %a</Command>
			<Argument> -a %p=%v1</Argument>
		</Set>
	</CfgMethod>

	<ParameterDef name="sh_pp" type="boolean" cfgmethod="shconf_prio" reboot="false"> 
		<Description><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="3">Enable Process Priority Problem</NLSCatalog></Description>
		<Set type="permanent">
			<Argument> -a %p=`if [ ''%v1 = '0' ]; then echo 'disable'; else echo 'enable'; fi`</Argument>
		</Set>
	</ParameterDef>

	<ParameterDef name="pp_errlog" type="boolean" cfgmethod="shconf_prio" reboot="false"> 
		<Description><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="4">Log Error in the Error Logging</NLSCatalog></Description>
		<Set type="permanent">
			<Argument> -a %p=`if [ ''%v1 = '0' ]; then echo 'disable'; else echo 'enable'; fi`</Argument>
		</Set>
	</ParameterDef>

	<ParameterDef name="pp_eto" type="integer" cfgmethod="shconf_prio" reboot="false"> 
		<Description><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="5">Detection Time-out</NLSCatalog></Description>
	</ParameterDef>

	<ParameterDef name="pp_eprio" type="integer" cfgmethod="shconf_prio" reboot="false"> 
		<Description><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="6">Process Priority</NLSCatalog></Description>
	</ParameterDef>

	<ParameterDef name="pp_warning" type="boolean" cfgmethod="shconf_prio" reboot="false"> 
		<Description><NLSCatalog catalog="artexcat.cat" setNum="42" msgNum="7">Display a warning message on a console</NLSCatalog></Description>

		<Set type="permanent">
