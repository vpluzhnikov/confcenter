<?xml version="1.0" encoding="UTF-8"?>
<Catalog id="gencopyParam" version="2.0">
 <ParameterDef name="genericFile" type="string" targetClass="file">
  <Get type="nextboot">
   <Command>/usr/bin/cat %t[file]</Command>
  </Get>

  <Get type="current">
   <Command>/usr/bin/cat %t[file]</Command>
  </Get>

  <Set type="permanent">
   <Command>/usr/bin/cat &gt; %t[file]</Command>
   <Stdin>%v1</Stdin>
  </Set>

  <Diff>
   <Command>/usr/bin/diff -w %f1 %f2; echo $?</Command>
  </Diff>
 </ParameterDef>
</Catalog>
