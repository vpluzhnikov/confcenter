      </Set>
    </ParameterDef>

    <ParameterDef name="netm_affinity" type="boolean" cfgmethod="no" reboot="true">
      <Description><NLSCommand command="/usr/sbin/no -h netm_affinity | /usr/bin/tail -n +2"/></Description>

      <Set type="permanent">
        <Argument>BOSBOOT=yes;RESTRICTED=yes;CMDLINE="$CMDLINE -o "%p"="%v1;</Argument>
      </Set>
      <Set type="nextboot">
        <Argument>BOSBOOT=yes;RESTRICTED=yes;CMDLINE="$CMDLINE -o "%p"="%v1;</Argument>
      </Set>

      <ConstraintDef>
	<Description><NLSCatalog catalog="artexcat.cat" setNum="38" msgNum="3">bosboot</NLSCatalog></Description>
	<BuiltIn>bosboot</BuiltIn>
      </ConstraintDef>
    </ParameterDef>

    <ParameterDef name="pseintrstack" type="integer">
      <Description><NLSCommand command="/usr/sbin/no -h pseintrstack | /usr/bin/tail -n +2"/></Description>
      <Get type="current">
        <Command>/usr/sbin/no -aF</Command>
        <Filter>/usr/bin/grep -v '= n/a$'</Filter>
        <Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
      </Get>

      <Get type="nextboot">
        <Command>/usr/sbin/no -r -aF</Command>
        <Filter>/usr/bin/grep -v '= n/a$'</Filter>
        <Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
      </Get>
    </ParameterDef>

    <ParameterDef name="use_isno" type="boolean" cfgmethod="no">
      <Description><NLSCommand command="/usr/sbin/no -h use_isno | /usr/bin/tail -n +2"/></Description>
      <Set type="permanent">
        <Argument>BOSBOOT=yes;RESTRICTED=yes;CMDLINE="$CMDLINE -o "%p"="%v1;</Argument>
      </Set>
      <Set type="nextboot">
        <Argument>BOSBOOT=yes;RESTRICTED=yes;CMDLINE="$CMDLINE -o "%p"="%v1;</Argument>
      </Set>
    </ParameterDef>

