<?xml version="1.0" encoding="UTF-8"?>
<Catalog id="vmoParam" version="2.1">

    <ShortDescription><NLSCatalog catalog="artexcat.cat" setNum="51" msgNum="1">vmo parameters</NLSCatalog></ShortDescription>
    <Description><NLSCatalog catalog="artexcat.cat" setNum="51" msgNum="2">Parameter definition for the vmo command</NLSCatalog></Description>

    <CfgMethod id="vmo">
	<Get type="current">
  	    <Command>/usr/sbin/vmo -aF</Command>
	    <Filter>/usr/bin/grep -v '= n/a$'</Filter>
	    <Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
	</Get>

	<Get type="nextboot">
	    <Command>/usr/sbin/vmo -r -aF</Command>
	    <Filter>/usr/bin/grep -v '= n/a$'</Filter>
	    <Mask name="1" value="2">[[:space:]]*(.*) = (.*)</Mask>
	</Get>

	<Set type="permanent">
	    <Command>%a { [[ -n $RESTRICTED ]] &amp;&amp; echo "yes"; [[ -n $BOSBOOT ]] &amp;&amp; echo "no"; } | vmo -p ${CMDLINE}</Command>
      	    <Argument>CMDLINE="$CMDLINE -o "%p"="%v1;</Argument>
	</Set>

	<Set type="nextboot">
	    <Command>%a { [[ -n $RESTRICTED ]] &amp;&amp; echo "yes"; [[ -n $BOSBOOT ]] &amp;&amp; echo "no"; } | vmo -r ${CMDLINE}</Command>
      	    <Argument>CMDLINE="$CMDLINE -o "%p"="%v1;</Argument>
	</Set>
    </CfgMethod>

