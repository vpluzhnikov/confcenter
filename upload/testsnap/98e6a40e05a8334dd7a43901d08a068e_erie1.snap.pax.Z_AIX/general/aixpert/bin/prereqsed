# Determine whether the machine is 64 bit or not.
KR=`/usr/sbin/bootinfo -K`

if  [ $KR -eq 64 ]
then
        exit 0          # Machine supports sedmgr
else
        exit 1          # Machine does not support sedmgr
fi

