#	OUTPUT			: None
#	Description		: This script checks whether CDE entry eixsts or not in /etc/inittab.

# Determine whether CDE has been removed from /etc/inittab
CDE=`/usr/bin/awk '/^[\t ]*dt:/{print "e"}' /etc/inittab`

if  [ "$CDE" = "" ]
then
	exit 0		# CDE entry has been removed from /etc/inittab
else
	exit 1		# CDE entry exists in /etc/inittab
fi
